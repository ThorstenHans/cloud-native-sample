{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "test": {
        "ClusterId": "cluster1",
        "Match": {
          "Path" : "/orders/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1" : {
            "Address" :"http://localhost:5000"
          }
        }
      }
    }
  }
}
