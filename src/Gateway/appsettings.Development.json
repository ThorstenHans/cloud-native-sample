{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "productdocs": {
        "ClusterId": "products",
        "Match": {
          "Path" : "/products/swagger/{**catch-all}"
        },
        "Transforms": [{ "PathRemovePrefix": "/products" }]
      },
      "products": {
        "ClusterId": "products",
        "Match": {
          "Path" : "/products/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "products": {
        "Destinations": {
          "products/destination1" : {
            "Address" :"http://localhost:5001"
          }
        }
      }
    }
  }
}
